%%
run('prioruncertainty.m')

totaltime = 0.5e6 * 365.25 * 24; % Half a million years (in hours)

SETUP = struct(                         ... 
    'L',            100,                 ...
    'W',            200,                ...
    'W_m',          40e3,        ...
    'dt',           7e4,                  ...
    't1',           totaltime*.25,                ...
    't2',           totaltime*.50,            ... %t1+351
    't3',           totaltime*.75,             ... %t1+385
    't4',           totaltime,            ... %t1+1285
    'saveInterval', 100,                  ...
    'showPlot',     false                ...
    ); 

maxTimeStep(SETUP,PRIOR)


%%
NAME = '/home/ammilten/Programs/FluvialRCM/Data/dev';
runMonteCarlo(SETUP, PRIOR, NAME, 1)